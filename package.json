{
    "name": "Neptune-Gremlin",
    "version": "0.1.1",
    "versionDate": "2021-06-18",
    "author": "hackolade",
    "engines": {
        "hackolade": "4.2.10",
        "hackoladePlugin": "1.0.0"
    },
    "contributes": {
        "target": {
            "applicationTarget": "Neptune-Gremlin",
            "title": "Neptune w/ Gremlin API",
