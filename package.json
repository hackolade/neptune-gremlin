{
    "name": "Neptune-Gremlin",
    "version": "0.1.8",
    "versionDate": "2022-07-20",
    "author": "hackolade",
    "engines": {
        "hackolade": "4.2.10",
        "hackoladePlugin": "1.0.0"
    },
    "contributes": {
        "target": {
            "applicationTarget": "Neptune-Gremlin",
            "title": "Neptune w/ Gremlin API",
            "versions": []
        },
        "features": {
            "jaySchemaHasRequiredPatternField": false,
            "nestedCollections": {
                "isVirtual": true,
                "hasMultipleParents": true
            },
            "enableComplexTypesNormalization": true,
            "graphDB": true,
            "relationships": {
                "connectOnEntityLevel": true,
                "hasData": true
            },
            "enableForwardEngineering": true,
            "enableReverseEngineering": true,
            "disableChoices": true,
            "disableMultipleTypes": true
        }
    },
    "description": "Hackolade plugin for Amazon Neptune with Gremlin API",
    "disabled": false,
    "dependencies": {
        "gremlin": "3.4.3",
        "tunnel-ssh": "4.1.4"
    },
    "lint-staged": {
        "*.{js,json}": "prettier --write"
    },
    "simple-git-hooks": {
        "pre-commit": "npx lint-staged",
        "pre-push": "npx eslint ."
    },
    "scripts": {
        "lint": "eslint . --max-warnings=0",
        "package": "node esbuild.package.js"
    },
    "devDependencies": {
        "@hackolade/hck-esbuild-plugins-pack": "0.0.1",
        "esbuild": "0.21.4",
        "esbuild-plugin-clean": "1.0.1",
        "eslint": "9.3.0",
        "eslint-config-prettier": "9.1.0",
        "eslint-plugin-prettier": "5.1.3",
        "lint-staged": "14.0.1",
        "prettier": "3.2.5",
        "simple-git-hooks": "2.11.1"
    }
}
